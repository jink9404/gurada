<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="StoreDAO">



	 <select id="getStore" parameterType="store" resultType="store">
		<![CDATA[
		select * from store where LOCATION LIKE '%'|| #{location} ||'%'
		]]>
	</select>
	
	<insert id="bookingInsert" parameterType="hashmap">
		insert into BOOKING(BOOKING_ID,FULL_NAME,STORE_ID,BOOKING_DATE,PHONE_NUMBER,TIME)
					values(seq_booking_no.nextval, #{name} ,#{storeId} , #{BookingDate} ,#{PhoneNumber},#{time})
	
	</insert>
	

	
 
</mapper>